<mapper namespace="com.goodee.spring.mapper.LoginMapper">
   
	<select id="selectKakaoId" parameterType="String" resultType="int">
		SELECT count(*) FROM SCOTT.TB_USER WHERE KAKAO_ID = #{kakaoId}
	</select>
	
	
   

</mapper>